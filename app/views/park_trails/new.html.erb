<%= form_with url: "/parks/#{@park.id}/trails", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :difficulty %>
  <%= form.text_field :difficulty %>
  <%= form.label :open %>
  <%= form.text_field :open %>
  <%= form.submit "Create Trail" %>
<% end %>
